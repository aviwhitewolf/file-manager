{
  "name": "@uncia/file-manager",
  "version": "0.1.0",
  "description": "A modern file manager UI component library built with React and Tailwind CSS",
  "author": "Uncia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uncia/file-manager.git"
  },
  "homepage": "https://github.com/uncia/file-manager#readme",
  "bugs": {
    "url": "https://github.com/uncia/file-manager/issues"
  },
  "keywords": [
    "react",
    "file-manager",
    "ui",
    "components",
    "tailwindcss",
    "radix-ui"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=4.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "radix-ui": "^1.4.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
